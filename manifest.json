{ "name": "Tailgate"
, "description": "Better Campfire Client for Chrome"
, "version": "0.0.1"
, "manifest_version": 2
, "minimum_chrome_version": "24"
, "app": { "background": { "scripts": [ "js/common.js", "js/background.js" ] } }
, "icons": { "16": "icons/logo-16.png", "128": "icons/logo-128.png" }
, "permissions": [ { "socket": [ "tcp-connect:localhost:3000"
                               , "tcp-connect:streaming.campfirenow.com:80"
                               ] }
                 , "storage"
                 , "https://*.campfirenow.com/*"
                 , "http://localhost:3000/*"
                 ]
}
